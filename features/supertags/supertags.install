<?php
/**
 * @file
 * Installs the Supertags feature.
 */

/**
 * Implements hook_enable().
 */
function supertags_enable() {
  $t = get_t();
  drupal_set_message($t('Supertags feature is now active on your site.'));
}

/**
 * Implements hook_disable().
 */
function supertags_disable() {
  $t = get_t();
  drupal_set_message($t('Supertags feature is now disabled on your site.'));
}
